@model Students.DAL.Models.Assessment
@{
    ViewData["Title"] = "Подробнее";
    Layout = "_Layout";
}
<body  style="color: #FFFFFF">
    <dl class="row" >
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Student)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Student.Id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Score)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Score)
        </dd>
    </dl>
    <div>
        <a class="btn btn-info,purpleButton"
           asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
        <a class="btn btn-info,purpleButton"
           asp-action="Index">Назад</a>
    </div>
</body>